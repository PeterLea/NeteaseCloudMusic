page{
  height: 100%;
  display: flex;
  flex-direction: column;
  overflow: auto;
}
songList{
  flex: 1;
}
.type-list-box{
  display: flex;
  justify-content: space-around;
}
.type-list-title{
  width: 82rpx;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  font-weight: 600;
  font-size: 28rpx;
  color: #333334;
}
.type-list-title-active{
  border-bottom: 4rpx solid #ff3a3a;
  color: #ff3a3a;
  width: 82rpx;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  font-weight: 600;
  font-size: 28rpx;
}
.playmenu,.chooseAll{
  width: 100%;
  display: flex;
  height: 100rpx;
  background: #fff;
  border-radius: 38rpx 38rpx 0 0;
  color: #000;
  align-items: center;
}
.playmenu>text{
  line-height: 1;
}
.icon-bofang{
  color: #333434;
  font-size: 40rpx;
  margin-left: 22rpx;
  margin-right: 20rpx;
}
.playall{
  font-weight: 549;
  font-size: 32rpx;
  flex:1;
}
.icon-gengduo1{
  color: #000;
  font-size: 26rpx;
}
.choosemore{
  font-size: 26rpx;
  margin-left: 8rpx;
  margin-right: 35rpx;
}
checkbox{
  width: 44rpx;
  height: 44rpx;
  margin:0 20rpx;
  border-radius: 50% !important;
}
checkbox .wx-checkbox-input {
  width: 44rpx;
  height: 44rpx;
  border-radius: 50%;
}
.checkall{
  flex:1;
}
.finish{
  margin-right: 47rpx;
  color: #ff3a3a;
}
.tips{
  height: 302rpx;
}
